
public class Fabrique {

	
	/**
	 * Construit une Fabrique vide.
	 */
	public Fabrique(){}
	
	
	public BarreLaterale creerBarreLaterale(){
		return new BarreLaterale(new Cellule(4.5,19.5,'e'), new Cellule(5.5,19.5,'e'), new Cellule(6.5,19.5,'e'));
	}
	
	public Mur creerMur(){
		return new Mur();
	}

}
