
public class Tri {
	
	public Tri(){}
	
	public static void triFusion(int[] tab,int d, int f){
		int milieu = (d+f)/2;
		if(d != f)
		fusion(triFusion(tab, d, milieu),triFusion(tab, milieu+1, f));
	}
	
	public int[] fusion(int deb , int fin){
		int tab1= new int[fin-deb+1]
		for(int i=deb ;i<fin)
	}
	
	public int getTab(){
		return tab[0];
	}
}
