
public class Tri {

	private int tab[];
	
	public Tri(int[] tab){
		this.tab = tab;
	}
	
	public int[] triFusion(int[] tab, int d, int f){
		int milieu = (d+f)/2;
		if(milieu <= 1)
			return tab;
		else
			return fusion(triFusion(tab, 1, milieu/2),triFusion(tab, milieu/2+1, tab.length));
	}
	
	public int[] fusion(int[] t1 , int[] t2){
		if()
	}
	
	public int getTab(){
		return tab[0];
	}
}
