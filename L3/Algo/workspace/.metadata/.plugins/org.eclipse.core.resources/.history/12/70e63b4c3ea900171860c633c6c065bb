
public class Tri {

	private int tab[];
	
	public Tri(int[] tab){
		this.tab = tab;
	}
	
	public int[] triFusion(int[] tab, int d, int f){
		if(tab.length <= 1)
			return tab;
		else
			return fusion(triFusion(tab, 0, tab.length/2-1),triFusion(tab, tab.length/2, tab.length-1));
	}
	
	public int[] fusion(int[] t1 , int[] t2){
		
	}
	
	public int getTab(){
		return tab[0];
	}
}
