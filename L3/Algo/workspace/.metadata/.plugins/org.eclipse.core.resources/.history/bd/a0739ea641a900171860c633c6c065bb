
public class Tri {

	private int tableau[];
	int d,f;
	
	public Tri(int[] tab,int d, int f){
		tableau = tab;
		this.d = d;
		this.f = f;
	}
	
	public int[] triFusion(){
		int milieu = (d+f)/2;
		if(milieu <= 1)
			return tab;
		else
			return fusion(triFusion(tab, 1, milieu/2),triFusion(tab, milieu/2+1, tab.length));
	}
	
	public int[] fusion(int[] tab){
		if()
	}
	
	public int getTab(){
		return tab[0];
	}
}
