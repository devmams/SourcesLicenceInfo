
public class Noeud {

	Noeud g;
	Noeud d;
	char elt;
	
	boolean estNul;
	
	public Noeud() {
		estNul = true;
	}
	
	public Noeud(Noeud g,Noeud d,char elt){
		this.g = g;
		this.d = d;
		this.elt = elt;
		estNul = false;
	}
	
	public boolean setEtat(){
		return !estNul;
	}
	
	public void ajouterEnfants(Noeud gauche,Noeud droit)
	{
		g = gauche;
		d = droit;
	}
	

}
