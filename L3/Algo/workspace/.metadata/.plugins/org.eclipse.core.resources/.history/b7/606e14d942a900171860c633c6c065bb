
public class Tri {

	private int tab[];
	int d,f;
	
	public Tri(int[] tab,int d, int f){
		this.tab = tab;
		this.d = d;
		this.f = f;
	}
	
	public int[] triFusion(){
		int milieu = (d+f)/2;
		if(milieu <= 1)
			return tab;
		else
			return fusion(triFusion(tab, deb, milieu),triFusion(tab, milieu+1, f));
	}
	
	public int[] fusion(int deb , int fin){
		int tab1= new int[fin-deb+1]
		for(int i=deb ;i<fin)
	}
	
	public int getTab(){
		return tab[0];
	}
}
