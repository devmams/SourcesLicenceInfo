
public class MainPrincipal {
	public static void main(String[] args) {
		Fabrique f = new Fabrique();
		Piece p = f.creerCarre();
		Plateau plat = new Plateau();
		plat.ajouterDebut(p);
		Piece tmp = p.versLeBas();
		plat.ajouter(p);
	
		System.out.println("------------");
		Piece pp = f.creerCarre();
		plat.ajouterDebut(pp);
	
	}

}
