
public class MainPrincipal {
	public static void main(String[] args) {
		int cpt = 0;
		Fabrique f = new Fabrique();
		Piece p = f.creerCarre();
		Plateau plat = new Plateau();
		plat.jouer(p);
		
		while(!bloque(p)){
			p = p.versLeBas();
		}
		
		Piece pp = f.creerCarre();
		plat.jouer(pp);
		
		
		
	
	}

}
