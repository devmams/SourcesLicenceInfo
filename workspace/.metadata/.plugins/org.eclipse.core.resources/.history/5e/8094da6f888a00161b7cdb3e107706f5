
public class Fabrique {
	private Piece creerCarre;
	private Piece creerBatonVertical;
	private Piece creerBatonHorizontal;
	public Fabrique(){
		this.creerCarre = new Piece(new Cellule(0.5,0.5),new Cellule(0.5,1.5),new Cellule(1.5,1.5),new Cellule(1.5,0.5));
		this.creerBatonVertical = new Piece(new Cellule(0.5,0.5),new Cellule(0.5,1.5),new Cellule(0.5,2.5),new Cellule(0.5,3.5));
		this.creerBatonHorizontal = new Piece(new Cellule(0.5,0.5),new Cellule(1.5,1.5),new Cellule(2.5,1.5),new Cellule(3.5,0.5));

	}

}